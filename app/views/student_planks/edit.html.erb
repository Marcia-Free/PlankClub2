<h1>Edit a Plank</h1>

<%= form_for @student_plank, :url => updated_plank_path(@student, @student_plank) do |f| %>
  <%= f.label :plank %>
  <%= f.collection_select :plank_id, Plank.all, :id, :planktype %>
  <br>
  <%= f.label :duration %>
  <%= f.text_field :duration %>
  <br>
  <%= f.submit %>
  <% end %>
